{
  "name": "code-generator-example",
  "version": "1.0.0",
  "author": "Apollo",
  "license": "MIT",
  "description": "An example of how to generate types based off of your GraphQL Schema.",
  "main": "src/main.ts",
  "scripts": {
    "generate": "graphql-codegen",
    "build": "tsc -w",
    "watch": "nodemon ./dist/index.js",
    "predev": "npm run generate",
    "dev": "concurrently \"npm run build\" \"npm run watch\"",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "@graphql-tools/utils": "^8.6.8",
    "apollo-datasource": "^3.3.1",
    "apollo-datasource-rest": "^3.5.2",
    "apollo-server": "^3.6.7",
    "fs": "^0.0.1-security",
    "graphql": "^16.3.0",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.4.1",
    "sqlite3": "^5.0.4",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-resolvers": "^2.4.3",
    "@types/node": "^17.0.13",
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0"
  }
}
